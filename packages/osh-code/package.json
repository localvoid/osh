{
  "name": "osh-code",
  "main": "dist/cjs/index.js",
  "module": "dist/js/index.js",
  "typings": "dist/typings/index.d.ts",
  "version": "0.2.1",
  "description": "Basic set of components for generating program code",
  "license": "MIT",
  "author": {
    "name": "Boris Kaul",
    "email": "localvoid@gmail.com",
    "url": "https://github.com/localvoid"
  },
  "keywords": [
    "osh"
  ],
  "homepage": "https://github.com/localvoid/osh",
  "repository": {
    "type": "git",
    "url": "https://github.com/localvoid/osh"
  },
  "bugs": {
    "url": "https://github.com/localvoid/osh/issues"
  },
  "github": "https://github.com/localvoid/osh",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepublish": "npm run dist",
    "dist": "npm run dist:es2015 && npm run dist:cjs",
    "dist:es2015": "../../node_modules/.bin/tsc --project ./tsconfig.build.es2015.json",
    "dist:cjs": "../../node_modules/.bin/tsc --project ./tsconfig.build.cjs.json",
    "lint": "../../node_modules/.bin/tslint --project ./tsconfig.json",
    "test": "../../node_modules/.bin/mocha --reporter mocha-iko-reporter -r ../../node_modules/ts-node/register __tests__/index.ts",
    "test:coverage": "../../node_modules/.bin/nyc ../../node_modules/.bin/mocha __tests__/index.ts"
  },
  "peerDependencies": {
    "osh": "^0.1.0"
  },
  "dependencies": {},
  "devDependencies": {},
  "nyc": {
    "require": [
      "../../node_modules/ts-node/register"
    ],
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true
  }
}